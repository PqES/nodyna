class VarDefinition
  include InferData
  attr_reader :name, :inferenceTypes, :inferenceValues
  
  def initialize(name)
    initInference()
    @name = name.to_s
  end
  
  def to_s
    return "#{@name}"
  end
  
  
  def infer(allClasses, className, methodName)
    return false
  end
  
  def recommend()

  end
  
  def mergeInfersFromVar(varDefinition)
    if(!varDefinition.nil?)
      newInfers = addInfers(varDefinition.inferenceTypes, varDefinition.inferenceValues)
      return newInfers
    end
    return false
  end

  def publicAddInfers(infers, values)
    return addInfers(infers, values)
  end
end
